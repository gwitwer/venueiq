	body
		#fb-root
		script.
			(function(d, s, id) {
				var js, fjs = d.getElementsByTagName(s)[0];
				if (d.getElementById(id)) return;
				js = d.createElement(s); js.id = id;
				js.src = 'https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v3.0&appId=1547931398624085&autoLogAppEvents=1';
				fjs.parentNode.insertBefore(js, fjs);
			}(document, 'script', 'facebook-jssdk'));
			window.fbAsyncInit = function() {
				FB.init({ appId: '1547931398624085', version: 'v3.0' });
				console.log('run init');
				setTimeout(function() {
					if (typeof(window.pageCode) !== 'undefined') {
						console.log('running page code');
						window.pageCode();
					}
				}, 0);
			};
